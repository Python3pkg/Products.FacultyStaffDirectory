<?xml version="1.0"?>
<atcttool
    xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <topic_indexes i18n:domain="plone">
  <index name="getRawPeople"
     description="The people assigned to a person grouping"
     enabled="True" friendlyName="People" 
     i18n:attributes="description; friendlyName">
   <criteria>ATReferenceCriterion</criteria>
  </index>
  <index name="getSortableName"
     description="The person's name, last name first"
     enabled="True" friendlyName="Full Name" 
     i18n:attributes="description; friendlyName">
   <criteria>ATListCriterion</criteria>
  </index>
 </topic_indexes>
 <topic_metadata i18n:domain="plone">
 </topic_metadata>
</atcttool>
